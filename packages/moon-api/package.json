    {
      "path": "dist/moon-api.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@simplewebauthn/typescript-types": "^8.0.0",
    "@size-limit/preset-small-lib": "^6.0.3",
    "@types/events": "^3.0.2",
    "husky": "^7.0.2",
    "size-limit": "^6.0.3",
    "tsdx": "^0.14.1",
    "tslib": "^2.3.1",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "@simplewebauthn/server": "^8.3.5"
  }
