{
  "name": "@moonup/moon-types",
  "version": "0.0.18",
  "author": "0xEwhal <ewhal@bytemasons.com>",
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "typings": "./dist/index.d.ts",
  "types": "./dist/index.d.ts",
  "exports": {
    "import": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    },
    "require": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "start": "tsup --watch",
    "build": "tsup",
    "test": "dts test",
    "lint": "dts lint",
    "prepare": "tsup",
    "size": "size-limit",
    "analyze": "size-limit --why"
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsdx lint"
    }
  },
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/moon-types.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/moon-types.esm.js",
      "limit": "10 KB"
    }
  ],
  "devDependencies": {
    "@size-limit/preset-small-lib": "^11.0.1",
    "@tsconfig/recommended": "^1.0.3",
    "@tsconfig/vite-react": "^3.0.0",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitest/coverage-v8": "^0.34.2",
    "husky": "^8.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "size-limit": "^11.0.1",
    "happy-dom": "^6.0.4",
    "tslib": "^2.6.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
